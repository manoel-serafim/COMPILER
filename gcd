/* Um programa para calcular o mdc
   segundo o algoritmo de Euclides. */
int gcd (int u, int v)
{       
	if (v == 0) return u ;
	else return gcd(v,u-u/v*v);
	/* u-u/v*v == u mod v */
}


void main(void)
{	
	
	int x; int y; int z;
	int vect[1];
	vect[0]=3;
	x= input();
	
	x=2;
	if (x==y) {
		z = x;
	} else {
		if (x>y) {
			z = x;
		} else {
			x=exe(x,y,z);/* ERRO AQUI, EXE NÃ‚O RETORNA NADA */
		}
	}
	x= gcd(z, y, x);
	x = input(); y = input();
	output(gcd(x,x,x));
}


